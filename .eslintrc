{
  "ecmaFeatures": {
    "modules": true,
    "blockBindings": true,
    "jsx": true
  },
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": 2,
    "curly": 2,
    "strict": 0,
    "quotes": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "dot-notation": 0,
    "no-unused-vars": [2, {"args": "none"}],
    "new-cap": 0,
    "radix": 2,
    "space-before-blocks": 2,
    "no-multiple-empty-lines": 2,
    "one-var": 2,
    "brace-style": [ 2, "1tbs", { "allowSingleLine": true }],
    "react/jsx-uses-vars": 1,
  },
  "globals": {
    "React": true
  }
}
