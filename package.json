{
  "name": "canon-react",
  "version": "0.1.5",
  "description": "Rackspace Canon components built with React",
  "repository": {
    "type": "git",
    "url": "https://github.com/rackerlabs/canon-react"
  },
  "scripts": {
    "prepublish": "./prepublish.sh",
    "build": "./node_modules/.bin/grunt build",
    "clean": "./node_modules/.bin/grunt clean",
    "demo-build": "./node_modules/.bin/grunt demo-build",
    "test-watch": "./node_modules/.bin/grunt watch 2>&1 >/dev/null & ./node_modules/karma/bin/karma start karma.dev.js",
    "test": "./node_modules/.bin/grunt build && ./node_modules/karma/bin/karma start karma.ci.js"
  },
  "keywords": [
    "react",
    "canon"
  ],
  "main": "transpiled/index.js",
  "directories": {
    "transpiled": "transpiled/",
    "dist": "dist/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "transpiled",
    "dist"
  ],
  "author": "Adam Pickeral <adam.pickeral@rackspace.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^5.5.8",
    "tether": "1.0.2"
  },
  "peerDependencies": {
    "react": ">=0.13.3"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babelify": "^6.1.2",
    "grunt": "~0.4.2",
    "grunt-babel": "^5.0.1",
    "grunt-browserify": "~1.3.0",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-jsxhint": "~0.5.0",
    "grunt-shell": "~0.6.4",
    "karma": "^0.12.22",
    "karma-browserify": "~3.0.1",
    "karma-chrome-launcher": "^0.1.4",
    "karma-failed-reporter": "0.0.2",
    "karma-jasmine": "^0.1.5",
    "karma-junit-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-react-jsx-preprocessor": "0.1.1",
    "karma-spec-reporter": "0.0.13",
    "react": "~0.13.2",
    "react-tools": "^0.13.3"
  }
}
